-- Sistema de tempo de farming
spawn(function()
    local startTime = nil
    
    local function format(value)
        return string.format('%02i', value)
    end
    
    local function convertToDHMS(seconds)
        local minutes = math.floor(seconds / 60)
        local secs = seconds % 60
        local hours = math.floor(minutes / 60)
        local mins = minutes % 60
        local days = math.floor(hours / 24)
        local hrs = hours % 24
        return format(days) .. ':' .. format(hrs) .. ':' .. format(mins) .. ':' .. format(secs)
    end
    
    while true do
        if _G.farmer or _G.farmer2 then
            if not startTime then
                startTime = os.time()
            end
            local elapsed = os.time() - startTime
            TimeFarmingLabel.Text = 'Time Farming: ' .. convertToDHMS(elapsed)
        end
        task.wait(1)
    end
end)

-- Sistema de Size Display
spawn(function()
    local suffixes = {'', 'K', 'M', 'B', 'T'}
    
    local function formatNumber(number, decimals)
        local exp = math.floor(math.log(math.max(1, math.abs(number)), 1000))
        local suffix = suffixes[1 + exp] or 'e+' .. exp
        local value = math.floor(number * (10 ^ decimals / 1000 ^ exp)) / 10 ^ decimals
        return string.format('%.' .. decimals .. 'f%s', value, suffix)
    end
    
    while true do
        SizeLabel.Text = 'Your Size: ' .. formatNumber(LocalPlayer.leaderstats.Strength.Value, 1)
        wait(1)
    end
end)

-- Sistema de Earned (ganhos)
spawn(function()
    local strengthStat = LocalPlayer.leaderstats.Strength
    local previousValue = strengthStat.Value
    local totalEarned = 0
    local suffixes = {'', 'K', 'M', 'B', 'T'}
    
    local function formatNumber(number, decimals)
        local exp = math.floor(math.log(math.max(1, math.abs(number)), 1000))
        local suffix = suffixes[1 + exp] or 'e+' .. exp
        local value = math.floor(number * (10 ^ decimals / 1000 ^ exp)) / 10 ^ decimals
        return string.format('%.' .. decimals .. 'f%s', value, suffix)
    end
    
    strengthStat:GetPropertyChangedSignal('Value'):Connect(function()
        totalEarned = totalEarned + (strengthStat.Value - previousValue)
        EarnedLabel.Text = 'You Earned: ' .. formatNumber(totalEarned, 1)
        previousValue = strengthStat.Value
    end)
end)

-- Sistema de Strength Display
spawn(function()
    while true do
        local playerName = LocalPlayer.Name
        wait(0.01)
        local strengthValue = game.ReplicatedStorage.Data[playerName].Strength.Value
        local formatted
        
        if strengthValue >= 1000000000000 then
            formatted = string.format('%.2fT', strengthValue / 1000000000000)
        elseif strengthValue >= 1000000000 then
            formatted = string.format('%.2fB', strengthValue / 1000000000)
        elseif strengthValue >= 1000000 then
            formatted = string.format('%.2fM', strengthValue / 1000000)
        elseif strengthValue >= 1000 then
            formatted = string.format('%.2fK', strengthValue / 1000)
        else
            formatted = strengthValue
        end
        
        StrengthLabel.Text = 'Strength: ' .. formatted .. ' | ' .. strengthValue
    end
end)

-- Sistema SPS/SPM/SPH/SPD/SPW
spawn(function()
    local suffixes = {'', 'K', 'M', 'B', 'T'}
    
    local function formatNumber(number, decimals)
        local exp = math.floor(math.log(math.max(1, math.abs(number)), 1000))
        local suffix = suffixes[1 + exp] or 'e+' .. exp
        local value = math.floor(number * (10 ^ decimals / 1000 ^ exp)) / 10 ^ decimals
        return string.format('%.' .. decimals .. 'f%s', value, suffix)
    end
    
    local strengthStat = LocalPlayer.leaderstats.Strength
    local previousValue = strengthStat.Value
    
    while true do
        wait(1)
        local currentValue = strengthStat.Value
        local gain = currentValue - previousValue
        
        SPSLabel.Text = 'SPS: ' .. formatNumber(gain, 1)
        SPMLabel.Text = 'SPM: ' .. formatNumber(gain * 60, 1)
        SPTMLabel.Text = 'SPTM: ' .. formatNumber(gain * 600, 1)
        SPHLabel.Text = 'SPH: ' .. formatNumber(gain * 3600, 1)
        SPDLabel.Text = 'SPD: ' .. formatNumber(gain * 86400, 1)
        SPWLabel.Text = 'SPW: ' .. formatNumber(gain * 604800, 1)
        
        previousValue = currentValue
    end
end)

