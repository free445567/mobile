-- Sistema de Farm com limite de Ping
Farm2Btn.MouseButton1Click:Connect(function()
    local waitTime = tonumber(Wait2Label.Text) or 1
    local stats = game:GetService('Stats')
    local pingLimit = tonumber(PingInput.Text)
    local farmingStopped = false
    local farmingActive = false
    
    local function startFarming()
        if not farmingActive then
            farmingActive = true
            spawn(function()
                pcall(function()
                    _G.farmer2 = true
                    while _G.farmer2 do
                        task.wait(waitTime)
                        for _, tool in pairs(LocalPlayer.Character:GetChildren()) do
                            if tool.Name == 'Double Weight' then
                                tool:Activate()
                            end
                        end
                    end
                end)
            end)
        end
    end
    
    local function stopFarming()
        if farmingActive then
            _G.farmer2 = false
            farmingActive = false
        end
    end
    
    startFarming()
    
    local checkInterval = waitTime
    
    while true do
        local currentPing = stats.Network.ServerStatsItem['Data Ping']:GetValue()
        
        if pingLimit <= currentPing and not farmingStopped then
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = 'Ping Limiter!',
                Text = 'Farm Stopped - Ping Over Limit',
                Duration = 3,
            })
            stopFarming()
            farmingStopped = true
        elseif currentPing < pingLimit and farmingStopped then
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = 'Ping Limiter!',
                Text = 'Farm Started - Ping Under Limit',
                Duration = 3,
            })
            startFarming()
            farmingStopped = false
        end
        
        task.wait(checkInterval)
    end
end)

StopFarm2Btn.MouseButton1Click:Connect(function()
    _G.farmer2 = false
end)

-- Botões de Equip/Unequip do Main2Frame (mesma lógica do Main1)
Equip2Btn.MouseButton1Click:Connect(function()
    _G.Equip = true
    local notificationSent = false
    
    if not _G.Equip then return end
    wait(0.631201)
    
    local equipped = 0
    for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do
        if tool.Name == 'Double Weight' then
            tool.Parent = LocalPlayer.Character
            equipped = equipped + 1
            if equipped >= 100 then break end
        end
    end
    
    local remaining = 0
    for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do
        if tool.Name == 'Double Weight' then
            remaining = remaining + 1
        end
    end
    
    if remaining == 0 and not notificationSent then
        wait(1)
        game:GetService('StarterGui'):SetCore('SendNotification', {
            Title = 'Equip',
            Text = 'Equipped All Weights. Enjoy!',
            Duration = 3,
        })
        _G.Equip = false
        notificationSent = true
    end
end)

Unequip2Btn.MouseButton1Click:Connect(function()
    _G.Equip = true
    local notificationSent = false
    
    if not _G.Equip then return end
    wait(0.631201)
    
    local unequipped = 0
    for _, tool in pairs(LocalPlayer.Character:GetChildren()) do
        if tool.Name == 'Double Weight' then
            tool.Parent = LocalPlayer.Backpack
            unequipped = unequipped + 1
            if unequipped >= 100 then break end
        end
    end
    
    local remaining = 0
    for _, tool in pairs(LocalPlayer.Character:GetChildren()) do
        if tool.Name == 'Double Weight' then
            remaining = remaining + 1
        end
    end
    
    if remaining == 0 and not notificationSent then
        wait(1)
        game:GetService('StarterGui'):SetCore('SendNotification', {
            Title = 'Equip',
            Text = 'Unequipped All Weights. Enjoy!',
            Duration = 3,
        })
        _G.Equip = false
        notificationSent = true
    end
end)

-- Atualização do Ping Limit Label
spawn(function()
    local suffixes = {'', 'K', 'M', 'B', 'T'}
    
    local function formatNumber(number, decimals)
        local exp = math.floor(math.log(math.max(1, math.abs(number)), 1000))
        local suffix = suffixes[1 + exp] or 'e+' .. exp
        local value = math.floor(number * (10 ^ decimals / 1000 ^ exp)) / 10 ^ decimals
        return string.format('%.' .. decimals .. 'f%s', value, suffix) = false
        notificationSent = true
    end
end)

-- Sistema de Unequip
UnequipBtn.MouseButton1Click:Connect(function()
    _G.Equip = true
    local notificationSent = false
    
    if not _G.Equip then return end
    
    wait(0.631201)
    
    local unequipped = 0
    for _, tool in pairs(LocalPlayer.Character:GetChildren()) do
        if tool.Name == 'Double Weight' then
            tool.Parent = LocalPlayer.Backpack
            unequipped = unequipped + 1
            if unequipped >= 100 then break end
        end
    end
    
    local remaining = 0
    for _, tool in pairs(LocalPlayer.Character:GetChildren()) do
        if tool.Name == 'Double Weight' then
            remaining = remaining + 1
        end
    end
    
    if remaining == 0 and not notificationSent then
        wait(1)
        game:GetService('StarterGui'):SetCore('SendNotification', {
            Title = 'Equip',
            Text = 'Unequipped All Weights. Enjoy!',
            Duration = 3,
        })
        _G.Equip
