TimeElapsedLabel.TextScaled = true
TimeElapsedLabel.TextStrokeTransparency = 0

-- Sistema de Dupe
DupeWeightsBtn.MouseButton1Click:Connect(function()
    local gamePassId = 5949054
    local delayPerDupe = 1
    
    local function promptGamePass(id)
        local marketplace = game:GetService('MarketplaceService')
        pcall(function()
            marketplace:SignalPromptGamePassPurchaseFinished(LocalPlayer, id, true)
        end)
    end
    
    local function countWeights()
        local count = 0
        for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do
            if tool.Name == 'Double Weight' then
                count = count + 1
            end
        end
        return count
    end
    
    local function updateTimeRemaining(label, seconds)
        local minutes = math.floor(seconds / 60)
        local secs = seconds % 60
        label.Text = string.format('Time Remaining: %d min %.2f sec', minutes, secs)
    end
    
    local function startDupeCountdown(timeRemaining, targetAmount)
        game:GetService('StarterGui'):SetCore('SendNotification', {
            Title = 'Auto Dupe!',
            Text = 'Started',
            Duration = 7,
        })
        
        while timeRemaining > 0 do
            wait(1)
            timeRemaining = timeRemaining - 1
            updateTimeRemaining(TimeElapsedLabel, timeRemaining)
            promptGamePass(gamePassId)
            
            if targetAmount <= countWeights() then
                break
            end
        end
        
        game:GetService('StarterGui'):SetCore('SendNotification', {
            Title = 'Countdown Completed!',
            Text = 'All weights processed!',
            Duration = 5,
        })
        
        TimeElapsedLabel.Text = 'Countdown Finished!'
    end
    
    local targetAmount = tonumber(DupeInput.Text) or 0
    local currentWeights = countWeights()
    
    if currentWeights < targetAmount then
        startDupeCountdown((targetAmount - currentWeights) * delayPerDupe, targetAmount)
    else
        TimeElapsedLabel.Text = 'No more weights needed!'
    end
end)

-- Atualização de contagem de weights no DupeFrame
spawn(function()
    while true do
        wait(0.1)
        local weightCount = 0
        
        for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do
            if tool.Name == 'Double Weight' then
                weightCount = weightCount + 1
            end
        end
        
        if weightCount == 0 then
            for _, tool in pairs(LocalPlayer.Character:GetChildren()) do
                if tool.Name == 'Double Weight' then
                    weightCount = weightCount + 1
                end
            end
        end
        
        WeightAmountLabel.Text = 'Weights: ' .. weightCount
        
        if weightCount == 9999999999999 then
            return
        end
    end
end)

