-- ==================================================================================
-- SEÇÃO 13: FUNÇÕES UTILITÁRIAS
-- ==================================================================================

-- Função para formatar números grandes com sufixos (K, M, B, T)
local function FormatarNumeroGrande(numero, casasDecimais)
    local sufixos = {'', 'K', 'M', 'B', 'T'}
    local magnitude = math.floor(math.log(math.max(1, math.abs(numero)), 1000))
    local sufixo = sufixos[1 + magnitude] or 'e+' .. magnitude
    local numeroFormatado = math.floor(numero * (10 ^ casasDecimais / 1000 ^ magnitude)) / 10 ^ casasDecimais
    
    return string.format('%.' .. casasDecimais .. 'f%s', numeroFormatado, sufixo)
end

-- Função para formatar tempo em formato DD:HH:MM:SS
local function FormatarTempo(segundos)
    local minutos = math.floor(segundos / 60)
    local segs = segundos % 60
    local horas = math.floor(minutos / 60)
    local mins = minutos % 60
    local dias = math.floor(horas / 24)
    local hrs = horas % 24
    
    return string.format('%02i:%02i:%02i:%02i', dias, hrs, mins, segs)
end

