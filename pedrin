-- SEÇÃO 19: FUNÇÕES DO PAINEL INFO - ATUALIZAÇÕES EM TEMPO REAL
-- ==================================================================================

-- Atualizar tempo de farm
coroutine.wrap(function()
    local tempoInicio = nil
    
    while true do
        if _G.farmer or _G.farmer2 then
            if not tempoInicio then
                tempoInicio = os.time()
            end
            
            local tempoDecorrido = os.time() - tempoInicio
            LabelTempoFarmando.Text = 'Time Farming: ' .. FormatarTempo(tempoDecorrido)
        end
        
        task.wait(1)
    end
end)()

-- Atualizar tamanho do jogador
coroutine.wrap(function()
    while true do
        local tamanho = JogadorLocal.leaderstats.Strength.Value
        LabelTamanhoJogador.Text = 'Your Size: ' .. FormatarNumeroGrande(tamanho, 1)
        wait(1)
    end
end)()

-- Atualizar ganhos totais
coroutine.wrap(function()
    local forcaInicial = JogadorLocal.leaderstats.Strength.Value
    local ganhoTotal = 0
    
    JogadorLocal.leaderstats.Strength:GetPropertyChangedSignal('Value'):Connect(function()
        local forcaAtual = JogadorLocal.leaderstats.Strength.Value
        ganhoTotal = ganhoTotal + (forcaAtual - forcaInicial)
        LabelGanhosTotal.Text = 'You Earned: ' .. FormatarNumeroGrande(ganhoTotal, 1)
        forcaInicial = forcaAtual
    end)
end)()

-- Atualizar força atual
coroutine.wrap(function()
    local ativo = true
    
    while ativo do
        local nomeJogador = JogadorLocal.Name
        wait(0.01)
        
        local forca = game.ReplicatedStorage.Data[nomeJogador].Strength.Value
        local forcaFormatada
        
        if forca >= 1000000000000 then
            forcaFormatada = string.format('%.2fT', forca / 1000000000000)
        elseif forca >= 1000000000 then
            forcaFormatada = string.format('%.2fB', forca / 1000000000)
        elseif forca >= 1000000 then
            forcaFormatada = string.format('%.2fM', forca / 1000000)
        elseif forca >= 1000 then
            forcaFormatada = string.format('%.2fK', forca / 1000)
        else
            forcaFormatada = tostring(forca)
        end
        
        LabelForcaAtual.Text = 'Strength: ' .. forcaFormatada .. ' | ' .. tostring(forca)
    end
end)()

-- Atualizar estatísticas de força por tempo
coroutine.wrap(function()
    local forcaAnterior = JogadorLocal.leaderstats.Strength.Value
    
    while true do
        wait(1)
        
        local forcaAtual = JogadorLocal.leaderstats.Strength.Value
        local ganhoSegundo = forcaAtual - forcaAnterior
        
        LabelForcaPorSegundo.Text = 'SPS: ' .. FormatarNumeroGrande(ganhoSegundo, 1)
        LabelForcaPorMinuto.Text = 'SPM: ' .. FormatarNumeroGrande(ganhoSegundo * 60, 1)
        LabelForcaPor10Minutos.Text = 'SPTM: ' .. FormatarNumeroGrande(ganhoSegundo * 600, 1)
        LabelForcaPorHora.Text = 'SPH: ' .. FormatarNumeroGrande(ganhoSegundo * 3600, 1)
        LabelForcaPorDia.Text = 'SPD: ' .. FormatarNumeroGrande(ganhoSegundo * 86400, 1)
        LabelForcaPorSemana.Text = 'SPW: ' .. FormatarNumeroGrande(ganhoSegundo * 604800, 1)
        
        forcaAnterior = forcaAtual
    end
end)()

-- ==================================================================================
-- SEÇÃO 20: FUNÇÕES DO PAINEL PLAYER - MONITORAMENTO DE JOGADORES
-- ==================================================================================

-- Monitorar jogadores farmando
coroutine.wrap(function()
    local dadosJogadores = {}
    local intervaloAtualizacao = 2
    
    local function AtualizarJogador(jogador)
        local label = ScrollFarmandoJogadores:FindFirstChild(jogador.Name)
        
        if not label then
            label = Instance.new('TextLabel')
            label.Name = jogador.Name
            label.Size = UDim2.new(0, 149, 0, 19)
            label.BackgroundColor3 = Color3.new(0, 0, 0)
            label.TextColor3 = Color3.new(1, 1, 1)
            label.Font = Enum.Font.FredokaOne
            label.TextSize = 18
            label.TextScaled = true
            label.Parent = ScrollFarmandoJogadores
        end
        
        local dadosJogador = game.ReplicatedStorage.Data:FindFirstChild(jogador.Name)
        
        if dadosJogador and dadosJogador:FindFirstChild('Strength') then
            local forcaAtual = dadosJogador.Strength.Value
            
            if not dadosJogadores[jogador.Name] then
                dadosJogadores[jogador.Name] = forcaAtual
            end
            
            if forcaAtual - dadosJogadores[jogador.Name] < 50 then
                label.Text = jogador.Name .. ' Not Farming'
                label.TextColor3 = Color3.new(1, 1, 1)
            else
                label.Text = jogador.Name .. ' Is Farming'
                label.TextColor3 = Color3.new(0, 1, 0)
            end
            
            dadosJogadores[jogador.Name] = forcaAtual
        else
            label.Text = jogador.Name .. ': No Strength Stat'
            dadosJogadores[jogador.Name] = nil
        end
    end
    
    local function AtualizarCanvasSize()
        local numJogadores = #game.Players:GetPlayers()
        ScrollFarmandoJogadores.CanvasSize = UDim2.new(0, 0, 0, numJogadores * 60)
    end
    
    local function AtualizarTodos()
        for _, jogador in pairs(game.Players:GetPlayers()) do
            AtualizarJogador(jogador)
        end
        AtualizarCanvasSize()
    end
    
    game.Players.PlayerAdded:Connect(function(jogador)
        AtualizarJogador(jogador)
    end)
    
    game.Players.PlayerRemoving:Connect(function(jogador)
        local label = ScrollFarmandoJogadores:FindFirstChild(jogador.Name)
        if label then
            label:Destroy()
        end
        dadosJogadores[jogador.Name] = nil
    end)
    
    while true do
        AtualizarTodos()
        wait(intervaloAtualizacao)
    end
end)()

-- Monitorar tamanho dos jogadores
coroutine.wrap(function()
    local tamanhosCacheados = {}
    
    local function AtualizarTamanho(jogador)
        local label = ScrollTamanhoJogadores:FindFirstChild(jogador.Name)
        
        if not label then
            label = Instance.new('TextLabel')
            label.Name = jogador.Name
            label.Size = UDim2.new(0, 149, 0, 19)
            label.BackgroundColor3 = Color3.new(0, 0, 0)
            label.TextColor3 = Color3.new(1, 1, 1)
            label.Font = Enum.Font.FredokaOne
            label.TextScaled = true
            label.TextSize = 18
            label.Parent = ScrollTamanhoJogadores
        end
        
        local leaderstats = jogador:FindFirstChild('leaderstats')
        
        if leaderstats and leaderstats:FindFirstChild('Strength') then
            local tamanho = leaderstats.Strength.Value
            
            if tamanhosCacheados[jogador.Name] ~= tamanho then
                label.Text = jogador.Name .. ': ' .. FormatarNumeroGrande(tamanho, 1)
                tamanhosCacheados[jogador.Name] = tamanho
            end
        else
            label.Text = jogador.Name .. ': No Strength Stat'
        end
    end
    
    local function AtualizarCanvasSize()
        local numJogadores = #game.Players:GetPlayers()
        ScrollTamanhoJogadores.CanvasSize = UDim2.new(0, 0, 0, numJogadores * 60)
    end
    
    local function AtualizarTodos()
        for _, jogador in pairs(game.Players:GetPlayers()) do
            AtualizarTamanho(jogador)
        end
        AtualizarCanvasSize()
    end
    
    game.Players.PlayerAdded:Connect(function(jogador)
        AtualizarTamanho(jogador)
        AtualizarCanvasSize()
    end)
    
    game.Players.PlayerRemoving:Connect(function(jogador)
        local label = ScrollTamanhoJogadores:FindFirstChild(jogador.Name)
        if label then
            label:Destroy()
        end
        tamanhosCacheados[jogador.Name] = nil
        AtualizarCanvasSize()
    end)
    
    while true do
        AtualizarTodos()
        wait(5)
    end
end)()

-- Monitorar força dos jogadores
coroutine.wrap(function()
    local forcasCacheadas = {}
    
    local function AtualizarForca(jogador)
        local label = ScrollForcaJogadores:FindFirstChild(jogador.Name)
        
        if not label then
            label = Instance.new('TextLabel')
            label.Name = jogador.Name
            label.Size = UDim2.new(0, 149, 0, 19)
            label.BackgroundColor3 = Color3.new(0, 0, 0)
            label.TextColor3 = Color3.new(1, 1, 1)
            label.Font = Enum.Font.FredokaOne
            label.TextScaled = true
            label.TextSize = 18
            label.Parent = ScrollForcaJogadores
        end
        
        local dadosJogador = game.ReplicatedStorage.Data:FindFirstChild(jogador.Name)
        
        if dadosJogador and dadosJogador:FindFirstChild('Strength') then
            local forca = dadosJogador.Strength.Value
            
            if forcasCacheadas[jogador.Name] ~= forca then
                label.Text = jogador.Name .. ': ' .. FormatarNumeroGrande(forca, 1)
                forcasCacheadas[jogador.Name] = forca
            end
        else
            label.Text = jogador.Name .. ': No Strength Stat'
        end
    end
    
    local function AtualizarCanvasSize()
        local numJogadores = #game.Players:GetPlayers()
        ScrollForcaJogadores.CanvasSize = UDim2.new(0, 0, 0, numJogadores * 60)
    end
    
    local function AtualizarTodos()
        for _, jogador in pairs(game.Players:GetPlayers()) do
            AtualizarForca(jogador)
        end
        AtualizarCanvasSize()
    end
    
    game.Players.PlayerAdded:Connect(function(jogador)
        AtualizarForca(jogador)
        AtualizarCanvasSize()
    end)
    
    game.Players.PlayerRemoving:Connect(function(jogador)
        local label = ScrollForcaJogadores:FindFirstChild(jogador.Name)
        if label then
            label:Destroy()
        end
        forcasCacheadas[jogador.Name] = nil
        AtualizarCanvasSize()
    end)
    
    while true do
        AtualizarTodos()
        wait(5)
    end
end)()

-- Monitorar pesos dos jogadores
coroutine.wrap(function()
    local function AtualizarPesos(jogador)
        local label = ScrollPesosJogadores:FindFirstChild(jogador.Name)
        
        if not label then
            label = Instance.new('TextLabel')
            label.Name = jogador.Name
            label.Size = UDim2.new(0, 149, 0, 19)
            label.BackgroundColor3 = Color3.new(0, 0, 0)
            label.TextColor3 = Color3.new(1, 1, 1)
            label.Font = Enum.Font.FredokaOne
            label.TextSize = 18
            label.TextScaled = true
            label.Parent = ScrollPesosJogadores
        end
        
        local temPesos = false
        local contagemPesos = 0
        local mochila = jogador:FindFirstChild('Backpack')
        local personagem = jogador:FindFirstChild('Character')
        
        if mochila then
            for _, item in pairs(mochila:GetChildren()) do
                if item.Name == 'Double Weight' then
                    contagemPesos = contagemPesos + 1
                    temPesos = true
                end
            end
        end
        
        if personagem then
            for _, item in pairs(personagem:GetChildren()) do
                if item.Name == 'Double Weight' then
                    contagemPesos = contagemPesos + 1
                    temPesos = true
                end
            end
        end
        
        if temPesos then
            label.Text = jogador.Name .. ': ' .. contagemPesos .. ' Double Weight(s)'
        else
            label.Text = jogador.Name .. ': Has No Double Weight'
        end
    end
    
    local function AtualizarCanvasSize()
        local numJogadores = #game.Players:GetPlayers()
        ScrollPesosJogadores.CanvasSize = UDim2.new(0, 0, 0, numJogadores * 60)
    end
    
    local function AtualizarTodos()
        for _, jogador in pairs(game.Players:GetPlayers()) do
            AtualizarPesos(jogador)
        end
        AtualizarCanvasSize()
    end
    
    local function ConfigurarMonitoramento(jogador)
        local mochila = jogador:WaitForChild('Backpack', 5)
        local personagem = jogador.Character or jogador.CharacterAdded:Wait()
        
        if mochila then
            mochila.ChildAdded:Connect(function(item)
                if item.Name == 'Double Weight' then
                    AtualizarPesos(jogador)
                end
            end)
            
            mochila.ChildRemoved:Connect(function(item)
                if item.Name == 'Double Weight' then
                    AtualizarPesos(jogador)
                end
            end)
        end
        
        if personagem then
            personagem.ChildAdded:Connect(function(item)
                if item.Name == 'Double Weight' then
                    AtualizarPesos(jogador)
                end
            end)
            
            personagem.ChildRemoved:Connect(function(item)
                if item.Name == 'Double Weight' then
                    AtualizarPesos(jogador)
                end
            end)
        end
    end
    
    game.Players.PlayerAdded:Connect(function(jogador)
        AtualizarPesos(jogador)
        ConfigurarMonitoramento(jogador)
        AtualizarCanvasSize()
    end)
    
    game.Players.PlayerRemoving:Connect(function(jogador)
        local label = ScrollPesosJogadores:FindFirstChild(jogador.Name)
        if label then
            label:Destroy()
        end
        AtualizarCanvasSize()
    end)
    
    for _, jogador in pairs(game.Players:GetPlayers()) do
        AtualizarPesos(jogador)
        ConfigurarMonitoramento(jogador)
    end
    AtualizarCanvasSize()
    
    while true do
        AtualizarTodos()
        wait(10)
    end
end)()

-- ==================================================================================
-- SEÇÃO 21: FUNÇÕES DO PAINEL DUPE
-- ==================================================================================

BotaoDuplicarPesos.MouseButton1Click:Connect(function()
    local gamePassIdDupe = 5949054
    local intervaloSegundos = 1
    
    local function SimularCompraGamePass(gamePassId)
        local MarketplaceService = game:GetService('MarketplaceService')
        pcall(function()
            MarketplaceService:SignalPromptGamePassPurchaseFinished(JogadorLocal, gamePassId, true)
        end)
    end
    
    local function ContarPesosAtuais()
        local contador = 0
        for _, item in pairs(JogadorLocal.Backpack:GetChildren()) do
            if item.Name == 'Double Weight' then
                contador = contador + 1
            end
        end
        return contador
    end
    
    local function AtualizarTempoRestante(label, segundosRestantes)
        local minutos = math.floor(segundosRestantes / 60)
        local segundos = segundosRestantes % 60
        label.Text = string.format('Time Remaining: %d min %.2f sec', minutos, segundos)
    end
    
    local function ExecutarDupe(segundosRestantes, quantidadeAlvo)
        game:GetService('StarterGui'):SetCore('SendNotification', {
            Title = 'Auto Dupe!',
            Text = 'Started',
            Duration = 7,
        })
        
        while segundosRestantes > 0 do
            wait(1)
            segundosRestantes = segundosRestantes - 1
            AtualizarTempoRestante(LabelTempoRestante, segundosRestantes)
            SimularCompraGamePass(gamePassIdDupe)
            
            if quantidadeAlvo <= ContarPesosAtuais() then
                break
            end
        end
        
        game:GetService('StarterGui'):SetCore('SendNotification', {
            Title = 'Countdown Completed!',
            Text = 'All weights processed!',
            Duration = 5,
        })
        
        LabelTempoRestante.Text = 'Countdown Finished!'
        print('Dupe Stopped')
    end
    
    local textoQuantidade = CaixaTextoDupe.Text
    local quantidadeDesejada = tonumber(textoQuantidade) or 0
    local pesosAtuais = ContarPesosAtuais()
    
    if pesosAtuais < quantidadeDesejada then
        ExecutarDupe((quantidadeDesejada - pesosAtuais) * intervaloSegundos, quantidadeDesejada)
    else
        print('You already have enough Double Weights.')
        LabelTempoRestante.Text = 'No more weights needed!'
    end
end)

-- Atualizar quantidade de pesos no painel Dupe
coroutine.wrap(function()
    while true do
        wait(0.1)
        
        local contagemPesos = 0
        
        for _, item in pairs(JogadorLocal.Backpack:GetChildren()) do
            if item.Name == 'Double Weight' then
                contagemPesos = contagemPesos + 1
            end
        end
        
        if contagemPesos == 0 then
            for _, item in pairs(JogadorLocal.Character:GetChildren()) do
                if item.Name == 'Double Weight' then
                    contagemPesos = contagemPesos + 1
                end
            end
        end
        
        LabelQuantidadePesos.Text = 'Weights: ' .. contagemPesos
        
        if contagemPesos >= 9999999999999 then
            return
        end
    end
end)()

-- ==================================================================================
-- SEÇÃO 22: FUNÇÕES DO PAINEL PING FARM
-- ==================================================================================

BotaoIniciarFarmPing.MouseButton1Click:Connect(function()
    local tempoEspera = tonumber(LabelTempoEsperaPing.Text) or 1
    local servicoStats = game:GetService('Stats')
    local limitePing = tonumber(CaixaTextoLimitePing.Text)
    local farmAtivo = false
    local sistemaIniciado = false
    
    local function IniciarFarm()
        if not sistemaIniciado then
            sistemaIniciado = true
            
            coroutine.wrap(function()
                pcall(function()
                    _G.farmer2 = true
                    
                    while _G.farmer2 do
                        task.wait(tempoEspera)
                        
                        for _, item in pairs(JogadorLocal.Character:GetChildren()) do
                            if item.Name == 'Double Weight' then
                                item:Activate()
                            end
                        end
                    end
                end)
            end)()
        end
    end
    
    local function PararFarm()
        if sistemaIniciado then
            _G.farmer2 = false
            sistemaIniciado = false
        end
    end
    
    IniciarFarm()
    
    local intervaloVerificacao = tempoEspera
    
    while true do
        local pingAtual = servicoStats.Network.ServerStatsItem['Data Ping']:GetValue()
        
        if limitePing <= pingAtual and not farmAtivo then
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = 'Ping Limiter!',
                Text = 'Farm Stopped - Ping Over Limit',
                Duration = 3,
            })
            PararFarm()
            farmAtivo = true
            
        elseif pingAtual < limitePing and farmAtivo then
            game:GetService('StarterGui'):SetCore('SendNotification', {
                Title = 'Ping Limiter!',
                Text = 'Farm Started - Ping Under Limit',
                Duration = 3,
            })
            IniciarFarm()
            farmAtivo = false
        end
        
        task.wait(intervaloVerificacao)
    end
end)

BotaoPararFarmPing.MouseButton1Click:Connect(function()
    _G.farmer2 = false
end)

-- Equipar pesos (Ping Farm)
BotaoEquiparPesosPing.MouseButton1Click:Connect(function()
    _G.Equip = true
    local notificacaoEnviada = false
    
    if not _G.Equip then
        return
    end
    
    wait(0.631201)
    
    local contador = 0
    for _, item in pairs(JogadorLocal.Backpack:GetChildren()) do
        if item.Name == 'Double Weight' then
            item.Parent = JogadorLocal.Character
            contador = contador + 1
            if contador >= 100 then
                break
            end
        end
    end
    
    local pesosRestantes = 0
    for _, item in pairs(JogadorLocal.Backpack:GetChildren()) do
        if item.Name == 'Double Weight' then
            pesosRestantes = pesosRestantes + 1
        end
    end
    
    if pesosRestantes == 0 and not notificacaoEnviada then
        wait(1)
        game:GetService('StarterGui'):SetCore('SendNotification', {
            Title = 'Equip',
            Text = 'Equipped All Weights. Enjoy!',
            Duration = 3,
        })
        _G.Equip = false
        notificacaoEnviada = true
    end
end)

-- Desequipar pesos (Ping Farm)
BotaoPararEquiparPesosPing.MouseButton1Click:Connect(function()
    _G.Equip = true
    local notificacaoEnviada = false
    
    if not _G.Equip then
        return
    end
    
    wait(0.631201)
    
    local contador = 0
    for _, item in pairs(JogadorLocal.Character:GetChildren()) do
        if item.Name == 'Double Weight' then
            item.Parent = JogadorLocal.Backpack
            contador = contador + 1
            if contador >= 100 then
                break
            end
        end
    end
    
    local pesosEquipados = 0
    for _, item in pairs(JogadorLocal.Character:GetChildren()) do
        if item.Name == 'Double Weight' then
            pesosEquipados = pesosEquipados + 1
        end
    end
    
    if pesosEquipados == 0 and not notificacaoEnviada then
        wait(1)
        game:GetService('StarterGui'):SetCore('SendNotification', {
            Title = 'Equip',
            Text = 'Equipped All Weights. Enjoy!',
            Duration = 3,
        })
        _G.Equip = false
        notificacaoEnviada = true
    end
end)

-- Atualizar label do limite de ping
coroutine.wrap(function()
    while true do
        local limitePing = tonumber(CaixaTextoLimitePing.Text)
        
        if limitePing then
            LabelLimitePing.Text = 'Ping Limit Set To: ' .. FormatarNumeroGrande(limitePing, 0)
        else
            LabelLimitePing.Text = 'Invalid Ping Limit'
        end
        
        wait(1)
    end
end)()

-- Atualizar contador de pesos (Ping Farm)
coroutine.wrap(function()
    local function ContarPesos()
        local contador = 0
        for _, item in pairs(JogadorLocal.Backpack:GetChildren()) do
            if item.Name == 'Double Weight' then
                contador = contador + 1
            end
        end
        if contador == 0 then
            for _, item in pairs(JogadorLocal.Character:GetChildren()) do
                if item.Name == 'Double Weight' then
                    contador = contador + 1
                end
            end
        end
        return contador
    end
    
    while true do
        LabelTempoEsperaPing.Text = tostring(0.000886 * ContarPesos())
        wait(1)
    end
end)()

-- Reduzir lag (Ping Farm) - mesma função que Farm Normal
BotaoReduzirLagPing.MouseButton1Click:Connect(function()
    BotaoReduzirLag.MouseButton1Click:Fire()
end)

-- ==================================================================================
-- SEÇÃO 23: TORNAR A GUI ARRASTÁVEL
-- ==================================================================================

local function TornarGUIArrastavel(frame)
    local UserInputService = game:GetService('UserInputService')
    local arrastando = false
    local inputArrastando = nil
    local posicaoInicial = nil
    local framePosicaoInicial = nil
    
    local function Atualizar(input)
        local delta = input.Position - posicaoInicial
        frame.Position = UDim2.new(
            framePosicaoInicial.X.Scale,
            framePosicaoInicial.X.Offset + delta.X,
            framePosicaoInicial.Y.Scale,
            framePosicaoInicial.Y.Offset + delta.Y
        )
    end
    
    local function IniciarArrastar(input)
        arrastando = true
        posicaoInicial = input.Position
        framePosicaoInicial = frame.Position
        
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                arrastando = false
            end
        end)
    end
    
    frame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or 
           input.UserInputType == Enum.UserInputType.Touch then
            IniciarArrastar(input)
        end
    end)
    
    frame.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or 
           input.UserInputType == Enum.UserInputType.Touch then
            inputArrastando = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input == inputArrastando and arrastando then
            Atualizar(input)
        end
    end)
end

TornarGUIArrastavel(FramePrincipal)

