-- Sistema de Farm Normal
FarmBtn.MouseButton1Click:Connect(function()
    local waitTime = tonumber(WaitLabel.Text)
    
    spawn(function()
        pcall(function()
            _G.farmer = true
            while _G.farmer do
                task.wait(waitTime)
                for _, tool in pairs(LocalPlayer.Character:GetChildren()) do
                    if tool.Name == 'Double Weight' and tool:IsA('Tool') then
                        tool:Activate()
                    end
                end
            end
        end)
    end)
end)

StopFarmBtn.MouseButton1Click:Connect(function()
    _G.farmer = false
end)

-- Sistema de Equip Automático
EquipBtn.MouseButton1Click:Connect(function()
    _G.Equip = true
    local notificationSent = false
    
    if not _G.Equip then return end
    
    wait(0.631201)
    
    local equipped = 0
    for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do
        if tool.Name == 'Double Weight' then
            tool.Parent = LocalPlayer.Character
            equipped = equipped + 1
            if equipped >= 100 then break end
        end
    end
    
    local remaining = 0
    for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do
        if tool.Name == 'Double Weight' then
            remaining = remaining + 1
        end
    end
    
    if remaining == 0 and not notificationSent then
        wait(1)
        game:GetService('StarterGui'):SetCore('SendNotification', {
            Title = 'Equip',
            Text = 'Equipped All Weights. Enjoy!',
            Duration = 3,
        })
        _G.Equip = false
        notificationSent = true
    end
end)

-- Sistema de Redução de Lag / Anti AFK
ReduceLagBtn.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet('https://pastebin.com/raw/mmEr60d5', true))()
    
    pcall(function() game.ReplicatedFirst.TourneyQ:Destroy() end)
    pcall(function() game:GetService('Lighting').NormalSky:Destroy() end)
    pcall(function() game:GetService('Players').LocalPlayer.PlayerGui.HUD:Destroy() end)
    
    local workspace = game.Workspace
    local lighting = game.Lighting
    local terrain = workspace.Terrain
    
    terrain.WaterWaveSize = 0
    terrain.WaterWaveSpeed = 0
    terrain.WaterReflectance = 0
    terrain.WaterTransparency = 0
    lighting.GlobalShadows = false
    lighting.FogEnd = 9000000000
    settings().Rendering.QualityLevel = 'Level01'
    
    for _, obj in pairs(game:GetDescendants()) do
        if obj:IsA('Part') or obj:IsA('Union') or obj:IsA('MeshPart') then
            obj.Reflectance = 0
        elseif obj:IsA('ParticleEmitter') or obj:IsA('Trail') then
            obj:Destroy()
        elseif obj:IsA('Explosion') then
            obj.BlastPressure = 0
            obj.BlastRadius = 0
        end
    end
    
    for _, effect in pairs(lighting:GetChildren()) do
        if effect:IsA('BlurEffect') or effect:IsA('SunRaysEffect') or 
           effect:IsA('ColorCorrectionEffect') or effect:IsA('BloomEffect') or 
           effect:IsA('DepthOfFieldEffect') then
            effect.Enabled = false
        end
    end
    
    sethiddenproperty(game.Lighting, 'Technology', 'Compatibility')
    
    game:GetService('StarterGui'):SetCore('SendNotification', {
        Title = 'ANTI AFK ACTIVE',
        Text = "This is so you won't get kicked",
        Duration = 7,
    })
    
    local VirtualUser = game:GetService('VirtualUser')
    LocalPlayer.Idled:connect(function()
        VirtualUser:Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
        wait(1)
        VirtualUser:Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
    end)
    
    for _, obj in pairs(workspace:GetChildren()) do
        if obj.Name ~= LocalPlayer.Name and obj.Name ~= 'Terrain' and 
           obj.Name ~= 'Baseplate' and obj.Name ~= 'Camera' then
            obj:Destroy()
        end
    end
    
    for _, obj in pairs(game:GetService('ReplicatedStorage'):GetChildren()) do
        if obj.Name ~= 'Data' then
            obj:Destroy()
        end
    end
    
    for _, obj in pairs(game:GetService('CorePackages'):GetChildren()) do
        obj:Destroy()
    end
end)

-- Atualização de contagem de weights
spawn(function()
    local function countWeights()
        local count = 0
        for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do
            if tool.Name == 'Double Weight' then
                count = count + 1
            end
        end
        if count == 0 then
            for _, tool in pairs(LocalPlayer.Character:GetChildren()) do
                if tool.Name == 'Double Weight' then
                    count = count + 1
                end
            end
        end
        return count
    end
    
    while true do
        WaitLabel.Text = 0.000886 * countWeights()
        wait(1)
    end
end)

