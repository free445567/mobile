-- NOTIFICAÇÕES (GAMEPASS REMOVIDO)
if JogadorLocal then
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Aguarde...",
        Text = "Carregando...",
        Duration = 7
    })
    wait(7)
    game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = "Bem-vindo " .. JogadorLocal.Name,
        Text = "Zaza Hub! By: abercke04",
        Duration = 3
    })
end
-- ==================================================================================
    })
end

