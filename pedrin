StrengthScroll.Size = UDim2.new(0, 147, 0, 81)
StrengthScroll.CanvasSize = UDim2.new(0, 0, 3, 0)
UIListLayout4.Parent = StrengthScroll
UIListLayout4.HorizontalAlignment = Enum.HorizontalAlignment.Center
UIListLayout4.SortOrder = Enum.SortOrder.LayoutOrder
UIListLayout4.Padding = UDim.new(0, 2)

