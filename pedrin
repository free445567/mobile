-- EVENTO: Iniciar Farm Normal
BotaoIniciarFarm.MouseButton1Click:Connect(function()
    local tempoEspera = tonumber(LabelTempoEspera.Text)
    
    coroutine.wrap(function()
        pcall(function()
            _G.farmer = true
            
            while _G.farmer do
                task.wait(tempoEspera)
                
                for _, itemPersonagem in pairs(JogadorLocal.Character:GetChildren()) do
                    if itemPersonagem.Name == 'Double Weight' and itemPersonagem:IsA('Tool') then
                        itemPersonagem:Activate()
                    end
                end
            end
        end)
    end)()
end)

-- EVENTO: Parar Farm Normal
BotaoPararFarm.MouseButton1Click:Connect(function()
    _G.farmer = false
end)

-- EVENTO: Equipar Pesos (Farm Normal)
BotaoEquiparPesos.MouseButton1Click:Connect(function()
    _G.Equip = true
    local notificacaoEnviada = false
    
    if not _G.Equip then
        return
    end
    
    wait(0.631201)
    
    -- Equipar até 100 Double Weights
    local contador = 0
    for _, itemMochila in pairs(JogadorLocal.Backpack:GetChildren()) do
        if itemMochila.Name == 'Double Weight' then
            itemMochila.Parent = JogadorLocal.Character
            contador = contador + 1
            if contador >= 100 then
                break
            end
        end
    end
    
    -- Verificar se ainda há pesos na mochila
    local pesosRestantes = 0
    for _, itemMochila in pairs(JogadorLocal.Backpack:GetChildren()) do
        if itemMochila.Name == 'Double Weight' then
            pesosRestantes = pesosRestantes + 1
        end
    end
    
    if pesosRestantes == 0 and not notificacaoEnviada then
        wait(1)
        game:GetService('StarterGui'):SetCore('SendNotification', {
            Title = 'Equip',
            Text = 'Equipped All Weights. Enjoy!',
            Duration = 3,
        })
        _G.Equip = false
        notificacaoEnviada = true
    end
end)

-- EVENTO: Desequipar Pesos (Farm Normal)
BotaoPararEquiparPesos.MouseButton1Click:Connect(function()
    _G.Equip = true
    local notificacaoEnviada = false
    
    if not _G.Equip then
        return
    end
    
    wait(0.631201)
    
    -- Desequipar até 100 Double Weights
    local contador = 0
    for _, itemPersonagem in pairs(JogadorLocal.Character:GetChildren()) do
        if itemPersonagem.Name == 'Double Weight' then
            itemPersonagem.Parent = JogadorLocal.Backpack
            contador = contador + 1
            if contador >= 100 then
                break
            end
        end
    end
    
    -- Verificar se ainda há pesos equipados
    local pesosEquipados = 0
    for _, itemPersonagem in pairs(JogadorLocal.Character:GetChildren()) do
        if itemPersonagem.Name == 'Double Weight' then
            pesosEquipados = pesosEquipados + 1
        end
    end
    
    if pesosEquipados == 0 and not notificacaoEnviada then
        wait(1)
        game:GetService('StarterGui'):SetCore('SendNotification', {
            Title = 'Equip',
            Text = 'Equipped All Weights. Enjoy!',
            Duration = 3,
        })
        _G.Equip = false
        notificacaoEnviada = true
    end
end)


-- EVENTO: Reduzir Lag e Anti-AFK (Farm Normal)
BotaoReduzirLag.MouseButton1Click:Connect(function()
    loadstring(game:HttpGet('https://pastebin.com/raw/mmEr60d5', true))()
    game.ReplicatedFirst.TourneyQ:Destroy()
    game:GetService('Lighting').NormalSky:Destroy()
    game:GetService('Players').LocalPlayer.PlayerGui.HUD:Destroy()
    
    local Workspace = game.Workspace
    local Lighting = game.Lighting
    local Terrain = Workspace.Terrain
    
    Terrain.WaterWaveSize = 0
    Terrain.WaterWaveSpeed = 0
    Terrain.WaterReflectance = 0
    Terrain.WaterTransparency = 0
    Lighting.GlobalShadows = false
    Lighting.FogEnd = 9000000000
    settings().Rendering.QualityLevel = 'Level01'
    
    for _, descendente in pairs(game:GetDescendants()) do
        if descendente:IsA('Part') or descendente:IsA('Union') or descendente:IsA('MeshPart') then
            descendente.Reflectance = 0
        elseif descendente:IsA('ParticleEmitter') or descendente:IsA('Trail') then
            descendente:Destroy()
        elseif descendente:IsA('Explosion') then
            descendente.BlastPressure = 0
            descendente.BlastRadius = 0
        end
    end
    
    for _, efeito in pairs(Lighting:GetChildren()) do
        if efeito:IsA('BlurEffect') or efeito:IsA('SunRaysEffect') or 
           efeito:IsA('ColorCorrectionEffect') or efeito:IsA('BloomEffect') or 
           efeito:IsA('DepthOfFieldEffect') then
            efeito.Enabled = false
        end
    end
    
    sethiddenproperty(game.Lighting, 'Technology', 'Compatibility')
    
    game:GetService('StarterGui'):SetCore('SendNotification', {
        Title = 'ANTI AFK ACTIVE',
        Text = "This is so you won't get kicked",
        Duration = 7,
    })
    
    local VirtualUser = game:GetService('VirtualUser')
    JogadorLocal.Idled:connect(function()
        VirtualUser:Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
        wait(1)
        VirtualUser:Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
    end)
    
    for _, objetoWorkspace in pairs(Workspace:GetChildren()) do
        if objetoWorkspace.Name ~= JogadorLocal.Name and 
           objetoWorkspace.Name ~= 'Terrain' and 
           objetoWorkspace.Name ~= 'Baseplate' and 
           objetoWorkspace.Name ~= 'Camera' then
            objetoWorkspace:Destroy()
        end
    end
    
    for _, objetoReplicatedStorage in pairs(game:GetService('ReplicatedStorage'):GetChildren()) do
        if objetoReplicatedStorage.Name ~= 'Data' then
            objetoReplicatedStorage:Destroy()
        end
    end
    
    for _, objetoCorePackages in pairs(game:GetService('CorePackages'):GetChildren()) do
        objetoCorePackages:Destroy()
    end
end)

-- Atualizar label de tempo de espera (Farm Normal)
coroutine.wrap(function()
    local function ContarPesos()
        local contador = 0
        
        for _, item in pairs(JogadorLocal.Backpack:GetChildren()) do
            if item.Name == 'Double Weight' then
                contador = contador + 1
            end
        end
        
        if contador == 0 then
            for _, item in pairs(JogadorLocal.Character:GetChildren()) do
                if item.Name == 'Double Weight' then
                    contador = contador + 1
                end
            end
        end
        
        return contador
    end
    
    while true do
        LabelTempoEspera.Text = tostring(0.000886 * ContarPesos())
        wait(1)
    end
end)()

