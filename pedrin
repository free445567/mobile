-- EVENTO: Click Teleport
BotaoClickTeleport.MouseButton1Click:Connect(function()
    game:GetService('StarterGui'):SetCore('SendNotification', {
        Title = 'Click TP Enabled!',
        Text = 'Tap Left Alt to TP to your mouse.',
        Duration = 10,
    })
    
    local mouse = JogadorLocal:GetMouse()
    
    game:GetService('UserInputService').InputBegan:connect(function(input, processado)
        if input.KeyCode == Enum.KeyCode.LeftAlt then
            local posicaoMouse = mouse.Hit + Vector3.new(0, 2.5, 0)
            local cframeTeleporte = CFrame.new(posicaoMouse.X, posicaoMouse.Y, posicaoMouse.Z)
            JogadorLocal.Character.HumanoidRootPart.CFrame = cframeTeleporte
        end
    end)
    
    print('Successfully Executed Script!')
end)

