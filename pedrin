Wait2Label.Text = '0'
Wait2Label.TextColor3 = Color3.fromRGB(85, 255, 0)
Wait2Label.TextScaled = true
Wait2Label.TextStrokeTransparency = 0
Wait2Label.TextTransparency = 1

spawn(function()
    local function countWeights()
        local count = 0
        for _, tool in pairs(LocalPlayer.Backpack:GetChildren()) do
            if tool.Name == 'Double Weight' then
                count = count + 1
            end
        end
        if count == 0 then
            for _, tool in pairs(LocalPlayer.Character:GetChildren()) do
                if tool.Name == 'Double Weight' then
                    count = count + 1
                end
            end
        end
        return count
    end
    
    while true do
        Wait2Label.Text = 0.000886 * countWeights()
        wait(1)
    end
end)

