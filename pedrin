WeightsScroll.Parent = PlayerFrame
WeightsScroll.Active = true
WeightsScroll.BackgroundColor3 = Color3.fromRGB(85, 255, 0)
WeightsScroll.Position = UDim2.new(0.0352680534, 0, 0.593615532, 0)
WeightsScroll.Size = UDim2.new(0, 147, 0, 81)
UIListLayout5.Parent = WeightsScroll
UIListLayout5.HorizontalAlignment = Enum.HorizontalAlignment.Center
UIListLayout5.SortOrder = Enum.SortOrder.LayoutOrder
UIListLayout5.Padding = UDim.new(0, 2)

-- Sistema de monitoramento de jogadores farming
spawn(function()
    local playerStrengthData = {}
    local updateInterval = 2
    
    local function updatePlayerFarming(player)
        local label = FarmingScroll:FindFirstChild(player.Name)
        
        if not label then
            label = Instance.new('TextLabel')
            label.Name = player.Name
            label.Size = UDim2.new(0, 149, 0, 19)
            label.BackgroundColor3 = Color3.new(0, 0, 0)
            label.TextColor3 = Color3.new(1, 1, 1)
            label.Font = Enum.Font.FredokaOne
            label.TextSize = 18
            label.TextScaled = true
            label.Parent = FarmingScroll
        end
        
        local playerData = game.ReplicatedStorage.Data:FindFirstChild(player.Name)
        
        if playerData and playerData:FindFirstChild('Strength') then
            local currentStrength = playerData.Strength.Value
            
            if not playerStrengthData[player.Name] then
                playerStrengthData[player.Name] = currentStrength
            end
            
            if currentStrength - playerStrengthData[player.Name] < 50 then
                label.Text = player.Name .. ' Not Farming'
                label.TextColor3 = Color3.new(1, 1, 1)
            else
                label.Text = player.Name .. ' Is Farming'
                label.TextColor3 = Color3.new(0, 1, 0)
            end
            
            playerStrengthData[player.Name] = currentStrength
        else
            label.Text = player.Name .. ': No Strength Stat'
            playerStrengthData[player.Name] = nil
        end
    end
    
    local function updateCanvasSize()
        local playerCount = #game.Players:GetPlayers()
        FarmingScroll.CanvasSize = UDim2.new(0, 0, 0, playerCount * 60)
    end
    
    local function updateAllPlayers()
        for _, player in pairs(game.Players:GetPlayers()) do
            updatePlayerFarming(player)
        end
        updateCanvasSize()
    end
    
    game.Players.PlayerAdded:Connect(function(player)
        updatePlayerFarming(player)
    end)
    
    game.Players.PlayerRemoving:Connect(function(player)
        local label = FarmingScroll:FindFirstChild(player.Name)
        if label then
            label:Destroy()
        end
        playerStrengthData[player.Name] = nil
    end)
    
    while true do
        updateAllPlayers()
        wait(updateInterval)
    end
end)

-- Sistema de monitoramento de Size dos jogadores
spawn(function()
    local suffixes = {'', 'K', 'M', 'B', 'T'}
    local playerSizeCache = {}
    
    local function formatNumber(number, decimals)
        local exp = math.floor(math.log(math.max(1, math.abs(number)), 1000))
        local suffix = suffixes[1 + exp] or 'e+' .. exp
        local value = math.floor(number * (10 ^ decimals / 1000 ^ exp)) / 10 ^ decimals
        return string.format('%.' .. decimals .. 'f%s', value, suffix)
    end
    
    local function updatePlayerSize(player)
        local label = SizeScroll:FindFirstChild(player.Name)
        
        if not label then
            label = Instance.new('TextLabel')
            label.Name = player.Name
            label.Size = UDim2.new(0, 149, 0, 19)
            label.BackgroundColor3 = Color3.new(0, 0, 0)
            label.TextColor3 = Color3.new(1, 1, 1)
            label.Font = Enum.Font.FredokaOne
            label.TextScaled = true
            label.TextSize = 18
            label.Parent = SizeScroll
        end
        
        local leaderstats = player:FindFirstChild('leaderstats')
        
        if leaderstats and leaderstats:FindFirstChild('Strength') then
            local sizeValue = leaderstats.Strength.Value
            
            if playerSizeCache[player.Name] ~= sizeValue then
                label.Text = player.Name .. ': ' .. formatNumber(sizeValue, 1)
                playerSizeCache[player.Name] = sizeValue
            end
        else
            label.Text = player.Name .. ': No Strength Stat'
        end
    end
    
    local function updateCanvasSize()
        local playerCount = #game.Players:GetPlayers()
        SizeScroll.CanvasSize = UDim2.new(0, 0, 0, playerCount * 60)
    end
    
    local function updateAllPlayers()
        for _, player in pairs(game.Players:GetPlayers()) do
            updatePlayerSize(player)
        end
        updateCanvasSize()
    end
    
    game.Players.PlayerAdded:Connect(function(player)
        updatePlayerSize(player)
        updateCanvasSize()
    end)
    
    game.Players.PlayerRemoving:Connect(function(player)
        local label = SizeScroll:FindFirstChild(player.Name)
        if label then
            label:Destroy()
        end
        playerSizeCache[player.Name] = nil
        updateCanvasSize()
    end)
    
    while true do
        updateAllPlayers()
        wait(5)
    end
end)

-- Sistema de monitoramento de Strength dos jogadores
spawn(function()
    local suffixes = {'', 'K', 'M', 'B', 'T'}
    local playerStrengthCache = {}
    
    local function formatNumber(number, decimals)
        local exp = math.floor(math.log(math.max(1, math.abs(number)), 1000))
        local suffix = suffixes[1 + exp] or 'e+' .. exp
        local value = math.floor(number * (10 ^ decimals / 1000 ^ exp)) / 10 ^ decimals
        return string.format('%.' .. decimals .. 'f%s', value, suffix)
    end
    
    local function updatePlayerStrength(player)
        local label = StrengthScroll:FindFirstChild(player.Name)
        
        if not label then
            label = Instance.new('TextLabel')
            label.Name = player.Name
            label.Size = UDim2.new(0, 149, 0, 19)
            label.BackgroundColor3 = Color3.new(0, 0, 0)
            label.TextColor3 = Color3.new(1, 1, 1)
            label.TextScaled = true
            label.Font = Enum.Font.FredokaOne
            label.TextSize = 18
            label.Parent = StrengthScroll
        end
        
        local playerData = game.ReplicatedStorage.Data:FindFirstChild(player.Name)
        
        if playerData and playerData:FindFirstChild('Strength') then
            local strengthValue = playerData.Strength.Value
            
            if playerStrengthCache[player.Name] ~= strengthValue then
                label.Text = player.Name .. ': ' .. formatNumber(strengthValue, 1)
                playerStrengthCache[player.Name] = strengthValue
            end
        else
            label.Text = player.Name .. ': No Strength Stat'
        end
    end
    
    local function updateCanvasSize()
        local playerCount = #game.Players:GetPlayers()
        StrengthScroll.CanvasSize = UDim2.new(0, 0, 0, playerCount * 60)
    end
    
    local function updateAllPlayers()
        for _, player in pairs(game.Players:GetPlayers()) do
            updatePlayerStrength(player)
        end
        updateCanvasSize()
    end
    
    game.Players.PlayerAdded:Connect(function(player)
        updatePlayerStrength(player)
        updateCanvasSize()
    end)
    
    game.Players.PlayerRemoving:Connect(function(player)
        local label = StrengthScroll:FindFirstChild(player.Name)
        if label then
            label:Destroy()
        end
        playerStrengthCache[player.Name] = nil
        updateCanvasSize()
    end)
    
    while true do
        updateAllPlayers()
        wait(5)
    end
end)

-- Sistema de monitoramento de Weights dos jogadores
spawn(function()
    local function updatePlayerWeights(player)
        local label = WeightsScroll:FindFirstChild(player.Name)
        
        if not label then
            label = Instance.new('TextLabel')
            label.Name = player.Name
            label.Size = UDim2.new(0, 149, 0, 19)
            label.BackgroundColor3 = Color3.new(0, 0, 0)
            label.TextColor3 = Color3.new(1, 1, 1)
            label.Font = Enum.Font.FredokaOne
            label.TextSize = 18
            label.TextScaled = true
            label.Parent = WeightsScroll
        end
        
        local hasWeights = false
        local weightCount = 0
        local backpack = player:FindFirstChild('Backpack')
        local character = player:FindFirstChild('Character')
        
        if backpack then
            for _, tool in pairs(backpack:GetChildren()) do
                if tool.Name == 'Double Weight' then
                    weightCount = weightCount + 1
                    hasWeights = true
                end
            end
        end
        
        if character then
            for _, tool in pairs(character:GetChildren()) do
                if tool.Name == 'Double Weight' then
                    weightCount = weightCount + 1
                    hasWeights = true
                end
            end
        end
        
        if hasWeights then
            label.Text = player.Name .. ': ' .. weightCount .. ' Double Weight(s)'
        else
            label.Text = player.Name .. ': Has No Double Weight'
        end
    end
    
    local function updateCanvasSize()
        local playerCount = #game.Players:GetPlayers()
        WeightsScroll.CanvasSize = UDim2.new(0, 0, 0, playerCount * 60)
    end
    
    local function updateAllPlayers()
        for _, player in pairs(game.Players:GetPlayers()) do
            updatePlayerWeights(player)
        end
        updateCanvasSize()
    end
    
    local function monitorPlayerInventory(player)
        local backpack = player:WaitForChild('Backpack', 5)
        local character = player.Character or player.CharacterAdded:Wait()
        
        if backpack then
            backpack.ChildAdded:Connect(function(child)
                if child.Name == 'Double Weight' then
                    updatePlayerWeights(player)
                end
            end)
            backpack.ChildRemoved:Connect(function(child)
                if child.Name == 'Double Weight' then
                    updatePlayerWeights(player)
                end
            end)
        end
        
        if character then
            character.ChildAdded:Connect(function(child)
                if child.Name == 'Double Weight' then
                    updatePlayerWeights(player)
                end
            end)
            character.ChildRemoved:Connect(function(child)
                if child.Name == 'Double Weight' then
                    updatePlayerWeights(player)
                end
            end)
        end
    end
    
    game.Players.PlayerAdded:Connect(function(player)
        updatePlayerWeights(player)
        monitorPlayerInventory(player)
        updateCanvasSize()
    end)
    
    game.Players.PlayerRemoving:Connect(function(player)
        local label = WeightsScroll:FindFirstChild(player.Name)
        if label then
            label:Destroy()
        end
        updateCanvasSize()
    end)
    
    for _, player in pairs(game.Players:GetPlayers()) do
        updatePlayerWeights(player)
        monitorPlayerInventory(player)
    end
    
    updateCanvasSize()
    
    while true do
        updateAllPlayers()
        wait(10)
    end
end)

